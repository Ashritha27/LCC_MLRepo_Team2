//Program to find the sum of numbers in a given range in C++
/*
Created on Novemeber 02, 2018
@author: ashrithashetty
*/
#include <iostream>

using namespace std;

int main()
{
    int min,max,sum=0;
    cout<<"Enter the limits";
    cin>>min;cin>>max;
    for(int i=min;i<=max;i++){
    sum+=i;
    }
    cout<<sum;
    return 0;
}
